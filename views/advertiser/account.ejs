document.getElementById('corbado-auth').addEventListener('click', async () => {
    try {
        // Trigger Windows Hello by creating a PublicKeyCredentialRequestOptions object
        const publicKeyCredentialRequestOptions = {
            challenge: new Uint8Array([/* Challenge generated by server */]), // Replace with actual challenge from your server
            timeout: 60000,
            // Use the correct domain for rpId
            rpId: window.location.hostname, // This dynamically sets rpId to the current domain
            userVerification: "required" // Require biometric verification (triggers Windows Hello)
        };

        // Trigger the authentication process (this will invoke Windows Hello if set up)
        const credential = await navigator.credentials.get({
            publicKey: publicKeyCredentialRequestOptions
        });

        // The user has successfully authenticated using Windows Hello
        console.log('Authenticated credential:', credential);

    } catch (error) {
        if (error.name === 'NotAllowedError') {
            alert('Windows Hello is not set up. Please set up Windows Hello to continue.');
        } else {
            console.error('Error during authentication:', error);
        }
    }
});

