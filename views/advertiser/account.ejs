// Function to detect the operating system
function detectOperatingSystem() {
    let operatingSystem = "Unknown OS";
    if (navigator.userAgent.indexOf("Win") != -1) operatingSystem = "Windows";
    if (navigator.userAgent.indexOf("Mac") != -1) operatingSystem = "MacOS";
    if (navigator.userAgent.indexOf("X11") != -1) operatingSystem = "UNIX";
    if (navigator.userAgent.indexOf("Linux") != -1) operatingSystem = "Linux";
    return operatingSystem;
}

// Function to detect the browser
function detectBrowser() {
    let userAgentString = navigator.userAgent;
    let browserName = "Unknown Browser";
    if (userAgentString.indexOf("Chrome") > -1) browserName = "Google Chrome";
    else if (userAgentString.indexOf("Safari") > -1) browserName = "Safari";
    else if (userAgentString.indexOf("Firefox") > -1) browserName = "Mozilla Firefox";
    else if (userAgentString.indexOf("MSIE") > -1 || userAgentString.indexOf("Trident/") > -1) browserName = "Internet Explorer";
    return browserName;
}

// Function to open the popup and display OS and Browser info
activeSessionsButton.addEventListener('click', () => {
    const os = detectOperatingSystem();
    const browser = detectBrowser();
    sessionInfo.textContent = `Operating System: ${os}\nBrowser: ${browser}`;

    sessionOverlay.style.display = 'block';
    sessionPopup.style.display = 'block';
});

